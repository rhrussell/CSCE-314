(define (triple-everything lst)
  (cond
      [(null? lst) '()]
      (else (cons(* 3 (car lst)) (triple-everything (cdr lst)))))
)

(define (main)
  (display (triple-everything '(1 2 3 4 5)))
  (newline)
  (display (triple-everything '(8 33.5 4/5 5-2i)))
  (exit)
)

(main)
